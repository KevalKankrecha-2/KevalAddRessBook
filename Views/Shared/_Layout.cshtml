                        </a>
                    </li>
                </ul>
            </li><!-- End city Nav -->
            <!-- contact nav -->
            <li class="nav-item">
                <a class="nav-link collapsed" data-bs-target="#charts-nav" data-bs-toggle="collapse" href="#">
                    <i class="bi bi-person-circle"></i><span>Contacts</span><i class="bi bi-chevron-down ms-auto"></i>
                </a>
                <ul id="charts-nav" class="nav-content collapse " data-bs-parent="#sidebar-nav">
                    <li>
                        <a asp-area="CON_Contact" asp-controller="CON_Contact" asp-action="OpenPage">
                            <i class="bi bi-plus-circle-fill" style="font-size: 16px"></i><span>Add Contact</span>
                        </a>
                    </li>
                    <li>
                        <a asp-area="CON_Contact" asp-controller="CON_Contact" asp-action="Index">
