                </a>
                <ul id="components-nav" class="nav-content collapse " data-bs-parent="#sidebar-nav">
                    <li>
                        <a asp-area="LOC_Country" asp-controller="LOC_Country" asp-action="OpenPage">
                            <i class="bi bi-plus-circle-fill" style="font-size: 16px"></i><span>Add Country</span>
                        </a>
                    </li>
                    <li>
                        <a  asp-area="LOC_Country" asp-controller="LOC_Country" asp-action="Index">
                            <i class="bi bi-kanban-fill" style="font-size: 16px"></i><span>Manage Country</span>
                        </a>
                    </li>
                </ul>
            </li><!-- End Components Nav -->
            <!-- State Nav bar -->
