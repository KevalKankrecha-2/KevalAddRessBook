                        </a>
                    </li>
                </ul>
            </li><!-- End contact Nav -->
            <!-- contact cat nav -->
            <li class="nav-item">
                <a class="nav-link collapsed" data-bs-target="#icons-nav" data-bs-toggle="collapse" href="#">
                    <i class="bi bi-bookmark-fill"></i><span>Contact Category</span><i class="bi bi-chevron-down ms-auto"></i>
                </a>
                <ul id="icons-nav" class="nav-content collapse " data-bs-parent="#sidebar-nav">
                    <li>
                        <a asp-area="MST_ContactCategory" asp-controller="MST_ContactCategory" asp-action="OpenPage">
                            <i class="bi bi-plus-circle-fill" style="font-size: 16px"></i><span>Add Contact Category</span>
                        </a>
                    </li>
                    <li>
                        <a asp-area="MST_ContactCategory" asp-controller="MST_ContactCategory" asp-action="Index">
                            <i class="bi bi-kanban-fill" style="font-size: 16px"></i><span>Manage Contact Category</span>
                        </a>
                    </li>
                </ul>
            </li><!-- End contact nav Nav -->


