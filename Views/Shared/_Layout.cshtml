                <ul id="forms-nav" class="nav-content collapse " data-bs-parent="#sidebar-nav">
                    <li>
                        <a asp-area="LOC_State" asp-controller="LOC_State" asp-action="OpenPage">
                            <i class="bi bi-plus-circle-fill" style="font-size: 16px"></i><span>Add State</span>
                        </a>
                    </li>
                    <li>
                        <a asp-area="LOC_State" asp-controller="LOC_State" asp-action="Index">
                            <i class="bi bi-kanban-fill" style="font-size: 16px"></i><span>Manage State</span>
                        </a>
                    </li>
                </ul>
            </li><!-- End state Nav -->
            <!-- city nav -->
            <li class="nav-item">
                <a class="nav-link collapsed" data-bs-target="#tables-nav" data-bs-toggle="collapse" href="#">
                    <i class="bi bi-building"></i><span>City</span><i class="bi bi-chevron-down ms-auto"></i>
                </a>
                <ul id="tables-nav" class="nav-content collapse " data-bs-parent="#sidebar-nav">
                    <li>
                        <a asp-area="LOC_City" asp-controller="LOC_City" asp-action="OpenPage">
